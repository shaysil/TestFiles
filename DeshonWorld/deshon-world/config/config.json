{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "search-header-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/searchHeader/SearchHeaderApplicationCustomizer.js",
          "manifest": "./src/extensions/searchHeader/SearchHeaderApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": "node_modules/jquery/dist/jquery.min.js",
    "jqueryui": "node_modules/jqueryui/jquery-ui.min.js"
  },
  "localizedResources": {
    "SearchHeaderApplicationCustomizerStrings": "lib/extensions/searchHeader/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}